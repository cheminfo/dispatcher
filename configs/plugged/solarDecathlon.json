{
  "devices": [
    {
      "type": "solarRoom",
      "description": "Solar decathlon room 1",
      "prefix": "$H",
      "id": "$H"
    },
    {
      "type": "solarRoom",
      "description": "Solar decathlon room 2",
      "prefix": "$J",
      "id": "$J"
    }
  ],
  "serialResponseTimeout": 250,
  "epochRefreshInterval": 2000,
  "port": {
    "regexp": "ttyUSB\\d+"
  },
  "baudrate": 38400,
  "sqlite": {
    "dir": "sqlite",
    "maxRecords": {
      "entry": 100000,
      "minute": 10000,
      "hour": 7200,
      "day": 300
    }
  }
}
